#!/usr/bin/env bash


echo "pwd: $PWD"

ls .
ls ../
ls ../../
ls /
ls ../../../



if [ -f bin/post_compile ]; then
    echo "-----> Running post-compile hook"
    chmod +x bin/post_compile
    sub_env bin/post_compile
    bash bin/post_compile
    echo "IN HOOK in if "
fi


echo "IN HOOK befire bash bin/post_compile"
bash bin/post_compile
echo "IN HOOK after  bash bin/post_compile"
